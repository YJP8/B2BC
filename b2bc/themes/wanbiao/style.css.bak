	background:url(images/kefu.png) no-repeat
}
#kf {
	z-index:5000;
	position:fixed;
	right:10px;
	top:190px;
	_position:absolute;
	_bottom:auto;
	_top:expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight-(parseInt(this.currentStyle.marginTop,10)||100)-(parseInt(this.currentStyle.marginBottom,10)||100)));
	width:78px;
	overflow:hidden;
	background-position:0 0
}
#kf a.close {
	position:absolute;
	top:0;
	right:0;
	width:15px;
	height:15px
}
#kf .context div.space {
	height:85px;
	cursor:pointer
}
#kf span {
	display:block;
	cursor:pointer;
	width:78px;
	height:40px;
	line-height:40px;
	clear:both;
	color:#fff;
	text-align:center
}
#kf span.kf_btn01 {
	background-position:0 -85px
}
#kf span.kf_btn01:hover,#kf span.kf_btn01Hover {
	background-position:-79px -85px
}
#kf span.kf_btn02 {
	background-position:0 -126px
}
#kf span.kf_btn02:hover,#kf span.kf_btn02Hover {
	background-position:-79px -126px
}
#kf span.kf_btn03 {
	background-position:0 -167px
}
#kf span.kf_btn03:hover,#kf span.kf_btn03Hover {
	background-position:-79px -167px
}
#kfs {
	display:none;
	z-index:5000;
	position:fixed;
	right:10px;
	top:188px;
	_position:absolute;
	_top:expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight-(parseInt(this.currentStyle.marginTop,200)||166)-(parseInt(this.currentStyle.marginBottom,200)||166)));
	30px;
	height:85px;
	background-position:-79px 0
}
#kfs a.show {
	display:block;
	width:78px;
	height:85px;
	text-decoration:none
}
#kf_talking {
